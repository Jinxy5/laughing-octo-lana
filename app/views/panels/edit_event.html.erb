<%= render partial: 'sidebar' %> 

<div class="col-sm-10">
	<h2>Calendars</h2>


 



						


		<table width="100%" border="1" cellspacing="10" cellpadding="10">
			<tr>
				<th scope="row">Public Calendar</th>
				<td><button type="button" class="btn btn-primary"><i class="fa fa-thumb-tack"></i> Add Event</button></td>
			</tr>
			<tr>
				<th scope="row">Admin Calendar</th>
				<td><button type="button" class="btn btn-success"><i class="fa fa-thumb-tack"></i> Add Event</button></td>
			</tr>
			<tr>
				<th scope="row">Rider Calendar</th>
				<td><button type="button" class="btn btn-info"><i class="fa fa-thumb-tack"></i> Add Event</button></td>
			</tr>
			<tr>
				<th scope="row">Midweek Calendar</th>
				<td><button type="button" class="btn btn-warning"><i class="fa fa-thumb-tack"></i> Add Event</button></td>
			</tr>
			<tr>
				<th scope="row">Coordinator Calendar</th>
				<td><button type="button" class="btn btn-danger"><i class="fa fa-thumb-tack"></i> Add Event</button></td>
			</tr>
	</table>


<div class="well2">
  <div id="datetimepicker1" class="input-append date">
    <input data-format="dd/MM/yyyy hh:mm:ss" type="text"></input>
    <span class="add-on">
      <i class="fa fa-calendar"></i> Start
    </span>
  </div>
</div>
<script type="text/javascript">
  $(function() {
    $('#datetimepicker1').datetimepicker({
      language: 'pt-BR'
    });
  });
</script>


</div>