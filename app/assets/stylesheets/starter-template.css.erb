/* Small devices (tablets, 768px and up) */

@media (min-width: 768px)  { 
    #logodiv h1 {
        font-size:30px; 
        line-height: 30px;
    } 
    #logodiv h2 {
        font-size:20px; 
        line-height: 20px;
    } 
    
    
    
    
       
}



/* Medium devices (desktops, 992px and up) */
@media (min-width: 992px) {    
	#logodiv h1 {
		font-size:40px; 
		line-height: 40px;
	} 
	#logodiv h2 {
		font-size:30px; 
		line-height: 30px;
	} 
	body {
	background-color: #F0F1EC;
	}
	#logodiv img {
		height: 114px;
		width: 114px;
}
}

